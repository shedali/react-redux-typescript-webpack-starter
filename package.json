{
  "name": "react-redux-typescript-webpack-starter",
  "version": "0.0.0",
  "description": "A general purpose template that can serve as a starter kit for a library or an app. Features TypeScript 2, @types, React, React Router, React Hot Loader 3, Redux, Webpack 2, Karma, Mocha, Enzyme, Chai, Sinon, Coverage, SASS, CSS Modules, Bootstrap.",
  "repository": {
    "type": "git",
    "url": "https://github.com/reanimatter/react-redux-typescript-webpack-starter"
  },
  "homepage": "https://github.com/reanimatter/react-redux-typescript-webpack-starter",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "webpack",
    "react-hot-loader",
    "hmr"
  ],
  "private": true,
  "main": "dist/bundles/bundle.umd.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build:vendor:dev": "NODE_ENV=development webpack --config webpack/vendor.config.js",
    "build:vendor:prod": "NODE_ENV=production webpack --config webpack/vendor.config.js",
    "prebuild:dev": "npm run build:vendor:dev",
    "build:dev": "NODE_ENV=development webpack --config webpack/app.config.js --profile --display-error-details",
    "prebuild:prod": "npm run build:vendor:prod",
    "build:prod": "NODE_ENV=production webpack --config webpack/app.config.js --profile --display-error-details",
    "compile:lib": "tsc --project lib.tsconfig.json",
    "build:lib:dev": "NODE_ENV=development webpack --config webpack/lib.config.js --profile --display-error-details",
    "build:lib:prod": "NODE_ENV=production webpack --config webpack/lib.config.js --profile --display-error-details",
    "prebuild:lib": "npm run clean",
    "build:lib": "npm run compile:lib && concurrently \"npm run build:lib:dev\" \"npm run build:lib:prod\"",
    "watch:dev": "npm run build:dev -- --watch",
    "start:hmr": "npm run server:dev:hmr",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "preserver:dev": "npm run clean && npm run build:vendor:dev",
    "server:dev": "NODE_ENV=development webpack-dev-server --config webpack/app.config.js --watch",
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "report-coverage": "codecov"
  },
  "author": "Maxim Lichmanov <reanimatter@gmail.com> (http://reanimatter.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/bootstrap": "^3.3.32",
    "@types/chai": "^3.4.34",
    "@types/classnames": "0.0.32",
    "@types/enzyme": "^2.5.39",
    "@types/history": "^4.5.0",
    "@types/jquery": "^2.0.34",
    "@types/lodash": "^4.14.44",
    "@types/mocha": "^2.2.34",
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "@types/react-hot-loader": "^3.0.1",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^4.0.4",
    "@types/react-router-dom": "^4.0.3",
    "@types/react-router-redux": "^5.0.0",
    "@types/redux": "^3.6.31",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "bootstrap-loader": "2.0.0-beta.16",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "colors": "^1.1.2",
    "concurrently": "^3.4.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.1",
    "progress-bar-webpack-plugin": "^1.9.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "remap-istanbul": "0.8.0",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^2.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.6.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0"
  },
  "files": [
    "dist"
  ]
}

{
  "name": "react-redux-typescript-webpack-starter",
  "version": "0.0.0",
  "description": "A general purpose template that can serve as a starter kit for a library or an app. Features TypeScript 2, @types, React, React Router, React Hot Loader 3, Redux, Webpack 2, Karma, Mocha, Enzyme, Chai, Sinon, Coverage, SASS, CSS Modules, Bootstrap.",
  "repository": {
    "type": "git",
    "url": "https://github.com/reanimatter/react-redux-typescript-webpack-starter"
  },
  "homepage": "https://github.com/reanimatter/react-redux-typescript-webpack-starter",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "webpack",
    "react-hot-loader",
    "hmr"
  ],
  "private": true,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build:vendor": "webpack --config webpack/vendor.config.js",
    "build:dev": "NODE_ENV=development webpack --config webpack.config.js --profile --display-error-details",
    "build:prod": "NODE_ENV=production webpack --config webpack.config.js --profile --display-error-details",
    "compile:lib": "tsc --project lib.tsconfig.json",
    "build:plugin:dev": "TARGET=plugin npm run build:dev",
    "build:plugin:prod": "TARGET=plugin npm run build:prod",
    "prebuild:plugin": "npm run clean",
    "build:plugin": "npm run compile:lib && concurrently --kill-others \"npm run build:plugin:dev\" \"npm run build:plugin:prod\"",
    "prebuild:lib:dev": "NODE_ENV=development TARGET=lib npm run build:vendor",
    "build:lib:dev": "TARGET=lib npm run build:dev",
    "prebuild:lib:prod": "NODE_ENV=production TARGET=lib npm run build:vendor",
    "build:lib:prod": "TARGET=lib npm run build:prod",
    "prebuild:lib": "npm run clean",
    "build:lib": "npm run compile:lib && concurrently --kill-others \"npm run build:lib:dev\" \"npm run build:lib:prod\"",
    "prebuild:app:dev": "NODE_ENV=development TARGET=app npm run build:vendor",
    "build:app:dev": "TARGET=app npm run build:dev",
    "prebuild:app:prod": "NODE_ENV=production TARGET=app npm run build:vendor",
    "build:app:prod": "TARGET=app npm run build:prod",
    "watch:dev": "npm run build:dev -- --watch",
    "start:hmr": "npm run server:dev:hmr",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "preserver:dev": "npm run clean && npm run build:vendor",
    "server:dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --watch",
    "test": "karma start",
    "watch:test": "npm run test -- --auto-watch --no-single-run"
  },
  "author": "Maxim Lichmanov <reanimatter@gmail.com> (http://reanimatter.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/bootstrap": "^3.3.32",
    "@types/chai": "^3.4.34",
    "@types/classnames": "0.0.31",
    "@types/enzyme": "^2.4.36",
    "@types/jquery": "^2.0.33",
    "@types/lodash": "^4.14.36",
    "@types/mocha": "^2.2.32",
    "@types/react": "^0.14.37",
    "@types/react-dom": "^0.14.17",
    "@types/react-redux": "^4.4.32",
    "@types/react-router": "^2.0.37",
    "@types/react-router-redux": "^4.0.34",
    "@types/redux": "^3.6.31",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "bootstrap-loader": "^2.0.0-beta.12",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.1",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.0",
    "node-sass": "^3.10.1",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^1.0.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.8",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "jquery": "^2.2.4",
    "lodash": "^4.16.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0"
  },
  "files": [
    "dist"
  ]
}
